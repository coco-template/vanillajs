{
  "name": "web-package",
  "version": "0.3.0",
  "description": "A seed to develop web package with typescript.",
  "module": "es/index.js",
  "main": "bundle/index.common.js",
  "typings": "native/index.d.ts",
  "files": [
    "es",
    "lib",
    "native"
  ],
  "scripts": {
    "prepublish": "npm-run-all -s clean compile",
    "prepush": "npm test",
    "precommit": "pretty-quick --staged",
    "dev": "webpack-dev-server",
    "clean": "rm -rf native es lib",
    "compile": "npm-run-all -s compile:*",
    "compile:native": "tsc --declaration --outDir native",
    "compile:esm": "babel src --source-maps --out-dir es --extensions '.js,.ts'",
    "compile:cjs": "babel src --source-maps --out-dir lib --extensions '.js,.ts' --plugins=@babel/plugin-transform-modules-commonjs",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coco-template/web-package.git"
  },
  "keywords": [
    "typescript",
    "package"
  ],
  "author": "huang.jian <hjj491229492@hotmail.com>",
  "contributors": [
    {
      "name": "huang.jian",
      "email": "hjj491229492@hotmail.com"
    },
    {
      "name": "yang.yuncai",
      "email": "383235388@qq.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coco-template/web-package/issues"
  },
  "homepage": "https://github.com/coco-template/web-package#readme",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@coco-platform/webpack-preset": "^1.0.0-2",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "webpack": "^4.20.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  }
}
