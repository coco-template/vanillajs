{
  "name": "web-package",
  "version": "0.2.0",
  "description": "NPM browser compatible package.",
  "module": "es/index.js",
  "main": "bundle/index.common.js",
  "typings": "native/index.d.ts",
  "files": [
    "es",
    "lib",
    "native"
  ],
  "scripts": {
    "prepublishOnly": "npm-run-all -s clean compile",
    "dev": "NODE_ENV=development webpack-dev-server --hot --inline",
    "clean": "rm -rf native es lib",
    "compile": "npm-run-all -s compile:*",
    "compile:native": "tsc --declaration --outDir native",
    "compile:esm": "babel src --source-maps --out-dir es --extensions '.js,.ts'",
    "compile:cjs": "babel src --source-maps --out-dir lib --extensions '.js,.ts' --plugins=@babel/plugin-transform-modules-commonjs",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coco-template/web-package.git"
  },
  "keywords": [
    "typescript",
    "package"
  ],
  "author": "huang.jian <hjj491229492@hotmail.com>",
  "contributors": [
    {
      "name": "huang.jian",
      "email": "hjj491229492@hotmail.com"
    },
    {
      "name": "yang.yuncai",
      "email": "383235388@qq.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coco-template/web-package/issues"
  },
  "homepage": "https://github.com/coco-template/web-package#readme",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/jest": "^24.0.6",
    "@types/node": "^12.0.2",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "coveralls": "^3.0.2",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.3",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "jest": "^24.1.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.4",
    "url-loader": "^2.0.0",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
