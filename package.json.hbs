{
  "name": "{{package}}",
  "version": "0.2.0",
  "description": "A lite seed to develop with typescript.",
  "module": "es/index.js",
  "main": "bundle/index.common.js",
  "typings": "native/index.d.ts",
  "files": ["es", "lib", "native"],
  "scripts": {
    "prepublish": "npm-run-all -s clean compile",
    "prepush": "npm test",
    "precommit": "pretty-quick --staged",
    "dev": "webpack-serve",
    "clean": "rm -rf native es lib",
    "compile": "npm-run-all -s compile:*",
    "compile:native": "tsc --declaration --outDir native",
    "compile:esm": "babel native --source-maps --out-dir es",
    "compile:es5":
      "babel native --source-maps --out-dir lib --plugins=transform-es2015-modules-commonjs",
    "test": "jest -c jest.config.js --coverage",
    "test:watch": "jest -c jest.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://{{provider}}/{{scope}}/{{repo}}.git"
  },
  "keywords": ["typescript", "package"],
  "author": "{{author}} <{{email}}>",
  "contributors": [
    {
      "name": "huang.jian",
      "email": "hjj491229492@hotmail.com"
    },
    {
      "name": "yang.yuncai",
      "email": "383235388@qq.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://{{provider}}/{{scope}}/{{repo}}/issues"
  },
  "homepage": "https://{{provider}}/{{scope}}/{{repo}}#readme",
  "dependencies": {},
  "devDependencies": {
    "@coco-platform/webpack-preset": "^0.2.1",
    "@types/jest": "^22.2.3",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.5.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack-serve": "^1.0.2"
  }
}
